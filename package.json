{
  "name": "create-react-app-seed",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "EXTEND_ESLINT=true react-app-rewired build",
    "ci": "CI=TRUE yarn build && CI=TRUE yarn test",
    "format:js": "prettier --write 'src/**/*.{js,jsx,json,css,scss,md}'",
    "start": "EXTEND_ESLINT=true react-app-rewired start",
    "test": "react-app-rewired test",
    "postinstall": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "package.json": "sort-package-json",
    "src/**/*.{js,jsx,json,css,scss,md}": [
      "prettier --write"
    ],
    "src/**/*.{js,jsx}": [
      "eslint --max-warnings=0"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "eslint-config-prettier": "^6.2.0",
    "eslint-plugin-prettier": "^3.1.0",
    "husky": "^5.1.3",
    "lint-staged": "^10.5.4",
    "prettier": "^1.18.2",
    "react": "^16.12.0",
    "react-app-rewired": "^2.1.5",
    "react-dom": "^16.12.0",
    "react-scripts": "3.4.0",
    "sort-package-json": "^1.49.0"
  }
}
